<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestMuAI Suite" parallel="classes" thread-count="2">

    <test name="Chrome_Windows_Test">
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="128.0"/>
        <parameter name="platform" value="Windows 10"/>
        <classes>
            <class name="tests.TestScenario1"/>
        </classes>
    </test>

    <test name="Edge_Mac_Test">
        <parameter name="browser" value="edge"/>
        <parameter name="browserVersion" value="127.0"/>
        <parameter name="platform" value="macOS Ventura"/>
        <classes>
            <class name="tests.TestScenario1"/>
        </classes>
    </test>

</suite>

